{
"permissions": [
    "http://*/*",
    "https://*/*"
],
"content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["toggle_nav_panel.js"]
    }
  ],

"background": {
    "scripts": ["background.js"],
    "persistent": false
},
"browser_action": {
  "default_icon": "fold.png"
},

"author": "Sinan Midillili",

"name":"Flip Estate",
"description":"Hide the nav bar on almost all wiki pages and the search bar on youtube so screen is less cluttered and neater.",
"version":"0.0.1.0",
"manifest_version":2,
"icons": {
  "16": "fold.png",
  "48": "fold.png",
  "128": "fold.png"
  }
}
