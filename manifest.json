{
"permissions": [
    "http://*/*",
    "https://*/*"
],
"background": {
    "scripts": ["background.js"],
    "persistent": false
},
"browser_action": {
  "default_icon": "fold.png"
},
"commands": {
  "_execute_browser_action": {
    "suggested_key": {
      "default": "Ctrl+Shift+E",
      "mac": "Ctrl+Shift+E"
    }
  }
},
"author": "Sinan Midillili",
"name":"Flip Estate",
"description":"Hide sidebars on Reddit, Wikipedia, Youtube so there isnt useless white space. Toggle by clicking on Icon or Command/Ctrl+Shift+E",
"version":"0.0.2.0",
"manifest_version":2,
"icons": {
  "16": "fold.png",
  "48": "fold.png",
  "128": "fold.png"
  }
}
